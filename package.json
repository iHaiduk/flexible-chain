{
  "name": "flexible-chain",
  "description": "This library will help you quickly and clearly build your code by building a chain of trees.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc -b",
    "lint": "tslint --format stylish --project tsconfig.json",
    "lint:fix": "yarn run lint --fix",
    "format": "prettier --write \"./src/**/*.{ts,tsx,js}\"",
    "test": "jest --config=jestconfig.json",
    "test:watch": "jest --config=jestconfig.json --watch",
    "test:coverage": "yarn jest --coverage",
    "coveralls": "yarn test:coverage && cat ./coverage/lcov.info | coveralls"
  },
  "keywords": [
    "flex",
    "chain",
    "flex chain",
    "object field tree",
    "js tree",
    "typescript chain",
    "typescript tree",
    "react",
    "react-native",
    "react-native-web",
    "react tree"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/iHaiduk/flexible-chain.git"
  },
  "author": "Ihor Haiduk",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/iHaiduk/flexible-chain/issues"
  },
  "homepage": "https://github.com/iHaiduk/flexible-chain#readme",
  "devDependencies": {
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-react": "^7.9.4",
    "@babel/preset-typescript": "^7.9.0",
    "@types/jest": "^25.2.1",
    "@types/react": "^16.9.34",
    "@types/react-test-renderer": "^16.9.2",
    "coveralls": "^3.1.0",
    "jest": "^26.0.1",
    "prettier": "^2.0.5",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-test-renderer": "^16.13.1",
    "ts-jest": "^25.5.0",
    "tslint": "^6.1.2",
    "tslint-config-prettier": "^1.18.0",
    "tslint-config-standard": "^9.0.0",
    "tslint-eslint-rules": "^5.4.0",
    "tslint-etc": "^1.10.1",
    "tslint-import-group-ordering": "^2.1.2",
    "tslint-microsoft-contrib": "^6.2.0",
    "tslint-origin-ordered-imports-rule": "^1.2.2",
    "tslint-sonarts": "^1.9.0",
    "typescript": "^3.8.3"
  },
  "publishConfig": {
    "access": "public"
  },
  "prepublishOnly": "npm test && npm run lint",
  "prepare": "npm run build",
  "preversion": "npm run lint",
  "version": "0.0.3",
  "postversion": "git push && git push --tags"
}
